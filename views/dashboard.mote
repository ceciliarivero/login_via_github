% user = current_user
<table>
  <tr>
    <td>Username:</td>
    <td>{{ user.username }}</td>
  </tr>
  <tr>
    <td>GitHub ID:</td>
    <td>{{ user.github_id }}</td>
  </tr>
  <tr>
    <td>Name:</td>
    <td>{{ user.name }}</td>
  </tr>
  <tr>
    <td>E-mail:</td>
    <td>{{ user.email }}</td>
  </tr>
    <td>Github account:</td>
    <td><a href="https://github.com/{{ user.username }}">https://github.com/{{ user.username }}</a></td>
  </tr>
</table>